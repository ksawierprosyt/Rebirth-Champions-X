local OrionLib = loadstring(game:HttpGet(('https://raw.githubusercontent.com/shlexware/Orion/main/source')))()
local Window = OrionLib:MakeWindow({Name = "[üçÄX8!] Rebirth Champions X",IntroEnabled = true, IntroText = "Void Hub", HidePremium = false, SaveConfig = true, ConfigFolder = "RebirthChampionsx"})

--Variables
getgenv().autoTap = true;

--Functions
function doTap()
      while autoTap == true do
         game:GetService("ReplicatedStorage"):WaitForChild("Events"):WaitForChild("Click3"):FireServer()
         wait()
         end
   end

local FarmTab = Window:MakeTab({
	Name = "Auto Farm",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

FarmTab:AddToggle({
	Name = "Auto Tap",
	Default = false,
	Callback = function(Value)
		_G.autoTap == Value
		doTap()
	end 
})
OrienLib:Init()
